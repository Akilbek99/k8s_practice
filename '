apiVersion: v1
kind: Pod
metadata:
  name: nginx1
  labels:
    app: nginx
    project: k8s_practice
    author: a-maxkamov
spec:
  containers:
    - name: nginx
      image: nginx:latest
      ports:
        - containerPort: 80
---
